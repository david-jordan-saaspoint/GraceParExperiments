<h1>Listing sfdctables</h1>

<head>
	<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.5.0/jquery.min.js"></script>
	<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.9/jquery-ui.min.js"></script>
	<script src="javascripts/fieldlist.js" type="text/javascript" charset="utf-8"></script>
</head>


<table>
  <tr>
    <th>Fieldname</th>
    <th>Fieldlabel</th>
    <th>Fieldtype</th>
    <th>Orgid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
 
<% @sfdctables.each do |sfdctable| %>
  <tr>
    <td><%= sfdctable.fieldname %></td>
     <td><%= sfdctable.fieldlabel %></td> 
    <td><%= sfdctable.fieldtype %></td>
      
    <td><%= sfdctable.orgid %></td>
    
        
    <td><%= link_to 'Show', sfdctable %></td>
    <td><%= link_to 'Edit', edit_sfdctable_path(sfdctable) %></td>
    <td><%= link_to 'Destroy', sfdctable, :confirm => 'Are you sure?', :method => :delete %></td>
    
  </tr>
<% end %>
</table>
 
<br />




<%= link_to 'Welcome Screen', :controller=> 'welcome', :action => 'index'%>
<p> <%= will_paginate @sfdctables %>
